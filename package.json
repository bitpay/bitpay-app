{
  "name": "bitpay",
  "version": "14.31.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --mode release",
    "ios": "react-native run-ios",
    "ios:device": "export NODE_OPTIONS=--openssl-legacy-provider && react-native run-ios --device",
    "ios:device:release": "export NODE_OPTIONS=--openssl-legacy-provider && react-native run-ios --configuration Release --device",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && react-native start",
    "test:coverage": "jest --coverage --testMatch='**/*.spec.{js,tsx,ts}' --config='jest.config.js'",
    "test:unit": "jest --watch --testMatch='**/*.spec.{js,tsx,ts}' --config='jest.config.js'",
    "test:cache": "jest --cache",
    "prebuild:android:debug": "yarn set:dev",
    "build:android:debug": "export NODE_OPTIONS=--openssl-legacy-provider && ./scripts/android-debug.sh",
    "postbuild:android:debug": "yarn reset:dev",
    "prebuild:android:release": "yarn set:prod",
    "build:android:release": "export NODE_OPTIONS=--openssl-legacy-provider && ./scripts/android-release.sh",
    "postbuild:android:release": "yarn reset:prod",
    "lint": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "postinstall": "./scripts/postinstall.sh",
    "clean": "react-native-clean-project",
    "pod:install": "command -v pod && (cd ios/ && bundle exec pod install && cd ..) || echo \"pod command not found\"",
    "splash": "react-native generate-bootsplash assets/img/logos/bitpay.png --background-color=\"#1A3B8B\" --logo-width 130",
    "postversion": "react-native-version",
    "set:dev": "export NODE_ENV=development && yarn set-all && node ./scripts/dev-overrides.js",
    "set:prod": "export NODE_ENV=production && yarn set-all",
    "set-all": "yarn set-braze && yarn set-google-maps && yarn set-allowed-url-prefixes",
    "set-allowed-url-prefixes": "node ./scripts/allowed-url-prefixes.js",
    "set-braze": "node ./scripts/braze-config.js",
    "set-google-maps": "node ./scripts/google-maps.js",
    "reset:dev": "export NODE_ENV=development && yarn reset-all && node ./scripts/dev-overrides-reset.js",
    "reset:prod": "export NODE_ENV=production && yarn reset-all",
    "reset-all": "yarn reset-braze && yarn reset-google-maps && yarn reset-allowed-url-prefixes",
    "reset-allowed-url-prefixes": "node ./scripts/allowed-url-prefixes.js reset",
    "reset-braze": "node ./scripts/braze-config-reset.js",
    "reset-google-maps": "node ./scripts/google-maps-reset.js",
    "translation:extract": "yarn run i18next 'src/**/*.{js,jsx,ts,tsx}'",
    "translation:upload": "crowdin upload sources --identity crowdin-credentials.yml",
    "translation:download": "crowdin download --identity crowdin-credentials.yml"
  },
